{
    "name": "alexeevdv/react-smpp",
    "description": "Async SMPP server and client implementations for ReactPHP.",
    "license": "MIT",
    "keywords": ["react", "reactphp", "smpp"],
    "autoload": {
        "psr-4": {
            "alexeevdv\\React\\Smpp\\" : "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.1",
        "ext-iconv": "*",
        "psr/log": "^1.1",
        "react/event-loop": "^1.1",
        "react/socket": "^1.2"
    },
    "require-dev": {
        "codeception/codeception": "^4.0",
        "codeception/module-asserts": "^1.3",
        "firehed/simplelogger": "^2.2",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        }
    }
}
